{
  "name": "@krdpt/maps-objects-ts",
  "version": "1.0.7",
  "description": "Object interfaces for maps",
  "main": "./src/index.ts",
  "module": "./src/index.mts",
  "types": "./src/index.d.ts",
  "scripts": {
    "run": "npx tsc && node dist/index.js",
    "build": "npx tsc --noEmit && tsup ./src/index.ts"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "directories": {
    "lib": "src"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "transport",
    "vehicles",
    "stops"
  ],
  "author": "Volan4ik",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "https://github.com/krdpt/maps-objects/"
  },
  "devDependencies": {
    "@swc/core": "^1.4.1",
    "ts-loader": "^9.5.1",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "webpack-cli": "^5.1.4"
  }
}
